aclocal --verbose 2>&1 && \
autoconf --verbose 2>&1 && \
autoheader --verbose 2>&1 && \
automake --add-missing --verbose 2>&1 && \
libtoolize --verbose 2>&1 && \
autoreconf -ivf 2>&1 && \
