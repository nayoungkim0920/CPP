.PHONY: configure-host

디버깅용으로 수정
ifeq ($(origin BUILD_CONFIG),undefined)
    BUILD_CONFIG = @BUILD_CONFIG@
endif

$(info =============BUILD_CONFIG:$(BUILD_CONFIG))
$(info =============srcdir:$(srcdir))

ifneq ($(BUILD_CONFIG),)
    $(foreach CONFIG, $(BUILD_CONFIG), \
        $(info =============CONFIG:$(CONFIG)::::$(srcdir)/config/$(CONFIG).mk);)
endif
