(오류)
install-fixincludes: installdirs
	CFLAGS_FOR_BUILD="-I/c/msys64/mingw64/include -I/mingw64/include -I/c/libidn2/gettext-0.22.5/libtextstyle/lib"
	CXXFLAGS_FOR_BUILD="-I/c/msys64/mingw64/lib/gcc/x86_64-w64-mingw32/14.2.0/include/c++ -I/c/msys64/mingw64/include -DDEBUG -DVERSION=1.0.0"
	CFLAGS="-I/c/msys64/mingw64/include -I/mingw64/include -I/c/libidn2/gettext-0.22.5/libtextstyle/lib"
	CXXFLAGS="-I/c/msys64/mingw64/lib/gcc/x86_64-w64-mingw32/14.2.0/include/c++ -I/c/msys64/mingw64/include -DDEBUG -DVERSION=1.0.0"
	@echo "install-fixincludes: installdirs=========================================="
	@echo "CFLAGS:$(CFLAGS)"
	@echo "CXXFLAGS:$(CXXFLAGS)"
	@echo "CFLAGS_FOR_BUILD:$(CFLAGS_FOR_BUILD)"
	@echo "CXXFLAGS_FOR_BUILD:$(CXXFLAGS_FOR_BUILD)"
	@echo "=========================================================================="
	@: $(MAKE); $(unstage)
	@r=`${PWD_COMMAND}`; export r; \  (에러가나는 구간)\
(출력)
install-fixincludes: installdirs==========================================
CFLAGS:-IC:/msys64/mingw64/include -I/mingw64/include -I/c/libidn2/gettext-0.22.5/libtextstyle/lib -O2
CXXFLAGS:-IC:/msys64/mingw64/lib/gcc/x86_64-w64-mingw32/14.2.0/include/c++ -I/c/msys64/mingw64/include -O2 -DDEBUG -DVERSION=1.0.0
