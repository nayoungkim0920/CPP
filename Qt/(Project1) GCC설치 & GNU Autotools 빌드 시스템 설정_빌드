(수정)
configure.ac 수정
# Define if we want to generate argz.h
AC_DEFINE([GL_GENERATE_ARGZ_H], [1], [Define if we want to generate argz.h])
# argz 필요한 함수들을 체크
AC_CHECK_FUNCS([argz_add argz_append argz_count argz_create_sep argz_insert argz_next argz_stringify])

Makefile.am 수정
# Here is where to add the argz module
# Begin Gnulib module argz
BUILT_SOURCES += argz.h

# We need the following in order to create <argz.h> when the system
# doesn't have one that works with the given compiler.
if GL_GENERATE_ARGZ_H
argz.h: argz.in.h $(top_builddir)/config.status
	$(gl_V_at)$(SED_HEADER_TO_AT_t) $(srcdir)/argz.in.h
	$(AM_V_at)mv $@-t $@
else
argz.h: $(top_builddir)/config.status
	rm -f $@
endif

MOSTLYCLEANFILES += argz.h argz.h-t

# Conditionally include argz.c
if GL_COND_OBJ_ARGZ
libtextstyle_la_SOURCES += lib/argz.c
endif

EXTRA_DIST += argz.in.h

# End Gnulib module argz

lib/Makefile.am 수정
## begin gnulib module argz

# Only generate argz.h if GL_GENERATE_ARGZ_H is defined
if GL_GENERATE_ARGZ_H
BUILT_SOURCES += argz.h

argz.h: argz.in.h $(top_builddir)/config.status
	$(gl_V_at)$(SED_HEADER_TO_AT_t) $(srcdir)/argz.in.h
	$(AM_V_at)mv $@-t $@
else
# Ensure that we remove argz.h if we do not generate it
argz.h: $(top_builddir)/config.status
	rm -f $@
endif

MOSTLYCLEANFILES += argz.h argz.h-t

# Conditionally compile argz.c if GL_COND_OBJ_ARGZ is defined
if GL_COND_OBJ_ARGZ
libgnu_a_SOURCES += argz.c
endif

EXTRA_DIST += argz.in.h

## end   gnulib module argz
