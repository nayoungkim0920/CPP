AM_CONDITIONAL([GL_GENERATE_ALLOCA_H], [test "x$generate_alloca_h" = "xyes"])
AM_CONDITIONAL([GL_GENERATE_ASSERT_H], [test "x$generate_assert_h" = "xyes"])
AM_CONDITIONAL([GL_COND_OBJ_ATEXIT], [test "x$cond_obj_atexit" = "xyes"])
AM_CONDITIONAL([GL_COND_OBJ_CANONICALIZE_LGPL], [test "x$cond_obj_canonicalize_lgpl" = "xyes"])
AM_CONDITIONAL([HAVE_ALLOCA_H], [test "x$have_alloca_h" = "xyes"])
AM_CONDITIONAL([HAVE_ASSERT_H], [test "x$have_assert_h" = "xyes"])

