if test $HAVE_ISNANL = 0 || test $REPLACE_ISNAN = 1; then   # AC_LIBOBJ -> AC_LIBOBJ # LIBOBJS -> AC_LIBOBJ
  AC_LIBOBJ([isnanl])
  gl_PREREQ_ISNANL
fi
