    if test -n "$ltstests_AC_LIBOBJ"; then # LIBOBJS -> AC_LIBOBJ
#       for i in `for i in $ltstests_LIBOBJS; do echo "$i"; done | sed -e "$sed_drop_objext" | sort | uniq`; do
      for i in `for i in $ltstests_AC_LIBOBJ; do echo "$i"; done | sed -e "$sed_drop_objext" | sort | uniq`; do # LIBOBJS -> AC_LIBOBJ
#     AC_SUBST([ltstests_LIBOBJS], [$ltstests_libobjs])
