libtool: install: base_file=`basename liblto_plugin.la`
libtool: install:  dlpath=`/bin/sh 2>&1 -c '. .libs/'liblto_plugin.la'i; echo liblto_plugin.dll'`
libtool: install:  dldir=/mingw64/libexec/gcc/x86_64-w64-mingw32/14.2.0/`dirname liblto_plugin.dll`
libtool: install:  test -d /mingw64/libexec/gcc/x86_64-w64-mingw32/14.2.0/. || mkdir -p /mingw64/libexec/gcc/x86_64-w64-mingw32/14.2.0/.
libtool: install:  /usr/bin/install -c .libs/liblto_plugin.dll /mingw64/libexec/gcc/x86_64-w64-mingw32/14.2.0/./liblto_plugin.dll
libtool: install:  chmod a+x /mingw64/libexec/gcc/x86_64-w64-mingw32/14.2.0/./liblto_plugin.dll
libtool: install:  if test -n '' && test -n 'strip --strip-unneeded'; then eval 'strip --strip-unneeded /mingw64/libexec/gcc/x86_64-w64-mingw32/14.2.0/./liblto_plugin.dll' || exit 0; fi
libtool: install: /usr/bin/install -c .libs/liblto_plugin.lai /mingw64/libexec/gcc/x86_64-w64-mingw32/14.2.0/liblto_plugin.la
mingw32-make[1]: Nothing to be done for 'install-data-am'.
mingw32-make[1]: Leaving directory 'C:/gcc-14.2.0/lto-plugin/build'

