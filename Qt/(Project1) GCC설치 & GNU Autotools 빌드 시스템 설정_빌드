if '/' in progname and not os.path.islink(progname):
    func_gnulib_dir()    
    prog = os.path.join(gnulib_dir, "gnulib-tool.py")
else:
    #prog = os.path.join(os.path.dirname(progname), "gnulib-tool.py")
    prog = os.path.join("C:\\gnulib\\", "gnulib-tool.py")

