# # 기존 자동화 파일 재생성
autoreconf --force --install
# Makefile.in을 자동으로 생성
automake --add-missing

/c/libidn2/gettext-0.22.5/gettext/gettext-runtime/gnulib-lib/configure.ac 수정
#C:\libidn2\gettext-0.22.5\gettext\gettext-runtime\intl-csharp
#C:\libidn2\gettext-0.22.5\gettext\gettext-runtime\gnulib-lib
#C:\libidn2\gettext-0.22.5\gettext\gettext-runtime\src
AC_CHECK_FILE([../intl-csharp/Makefile.in], [AC_MSG_RESULT([found])], [AC_MSG_RESULT([not found])])
AC_CHECK_FILE([../gnulib-lib/Makefile.in], [AC_MSG_RESULT([found])], [AC_MSG_RESULT([not found])])
AC_CHECK_FILE([../src/Makefile.in], [AC_MSG_RESULT([found])], [AC_MSG_RESULT([not found])])

AC_CHECK_FILE([getopt.h], [AM_CONDITIONAL([GL_COND_OBJ_GETOPT], [true])], [AM_CONDITIONAL([GL_COND_OBJ_GETOPT], [false])])
AC_CHECK_FILE([getprogname.h], [AM_CONDITIONAL([GL_COND_OBJ_GETPROGNAME], [true])], [AM_CONDITIONAL([GL_COND_OBJ_GETPROGNAME], [false])])
AC_CHECK_FUNCS([snprintf], [AM_CONDITIONAL([GL_COND_OBJ_SNPRINTF], [true])], [AM_CONDITIONAL([GL_COND_OBJ_SNPRINTF], [false])])
AC_CHECK_FUNCS([strdup], [AM_CONDITIONAL([GL_COND_OBJ_STRDUP], [true])], [AM_CONDITIONAL([GL_COND_OBJ_STRDUP], [false])])
AC_CHECK_FUNCS([iconv_open], [AM_CONDITIONAL([GL_COND_OBJ_ICONV_OPEN], [true])], [AM_CONDITIONAL([GL_COND_OBJ_ICONV_OPEN], [false])])
AC_CHECK_FUNCS([wcwidth], [AM_CONDITIONAL([GL_COND_OBJ_WCWIDTH], [true])], [AM_CONDITIONAL([GL_COND_OBJ_WCWIDTH], [false])])
# Gnulib를 초기화하는 경우 추가할 필요가 있다면 아래와 같은 구문을 사용합니다.
# AC_LIBOBJ([getopt])

/c/libidn2/gettext-0.22.5/gettext/gettext-runtime/gnulib-lib/Makefile.am 수정
# 기본적인 소스 파일 설정
libobj_la_SOURCES = \
    main.c \  # 기본 소스 파일들

if GL_COND_OBJ_GETOPT
libobj_la_SOURCES += getopt.c
endif

if GL_COND_OBJ_GETPROGNAME
libobj_la_SOURCES += getprogname.c
endif

if GL_COND_OBJ_SNPRINTF
libobj_la_SOURCES += snprintf.c
endif

if GL_COND_OBJ_STRDUP
libobj_la_SOURCES += strdup.c
endif

if GL_COND_OBJ_ICONV_OPEN
libobj_la_SOURCES += iconv.c
endif

if GL_COND_OBJ_WCWIDTH
libobj_la_SOURCES += wcwidth.c
endif

# 라이브러리 및 링크 설정
libobj_la_LDFLAGS = -no-undefined
libobj_la_LIBADD =

(오류)
Makefile.am:729: error: GL_GENERATE_ICONV_H does not appear in AM_CONDITIONAL
(수정)
Makefile.am에서 GL_GENERATE_ICONV_H 조건을 사용하려고 했지만, 
configure.ac에서 이 조건이 정의되지 않았거나 제대로 연결되지 않았기 때문에 발생
configure.ac에서 GL_GENERATE_ICONV_H 조건을 정의하고, 그 후 Makefile.am에서 이를 참조하는 방식으로 수정
/c/libidn2/gettext-0.22.5/gettext/gettext-runtime/gnulib-lib/configure.ac 수정
AC_ARG_ENABLE([generate-iconv-h],
  [AS_HELP_STRING([--enable-generate-iconv-h], [Enable generation of iconv.h])],
  [generate_iconv_h=$enableval], [generate_iconv_h=no])
AM_CONDITIONAL(GL_GENERATE_ICONV_H, test "$generate_iconv_h" = "yes")

AC_ARG_ENABLE([cond-obj-iconv],
  [AS_HELP_STRING([--enable-cond-obj-iconv], [Enable cond-obj-iconv])],
  [cond_obj_iconv=$enableval], [cond_obj_iconv=no])
AM_CONDITIONAL(GL_COND_OBJ_ICONV, test "$cond_obj_iconv" = "yes")

AC_ARG_ENABLE([cond-obj-iswblank],
  [AS_HELP_STRING([--enable-cond-obj-iswblank], [Enable cond-obj-iswblank])],
  [cond_obj_iswblank=$enableval], [cond_obj_iswblank=no])
AM_CONDITIONAL(GL_COND_OBJ_ISWBLANK, test "$cond_obj_iswblank" = "yes")

