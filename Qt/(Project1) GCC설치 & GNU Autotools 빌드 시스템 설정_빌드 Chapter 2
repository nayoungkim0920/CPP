sed -i '/# The user is always right./a \
if command -v uname >/dev/null 2>&1; then\
    uname_output=$(uname -s)\
    case "$uname_output" in\
        *CYGWIN*|*MINGW*|*MSYS*|*WIN*) PATH_SEPARATOR=";";;\
        *) PATH_SEPARATOR=":";;\
    esac\
    echo "PATH_SEPARATOR is set to $PATH_SEPARATOR"\
else\
    echo "Warning: '\''uname'\'' command not found. Setting default PATH_SEPARATOR."\
    PATH_SEPARATOR=":"\
fi' configure
