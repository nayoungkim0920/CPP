# notepad change_LIBOBJS_to_AC_LIBOBJ.sh

#!/bin/bash

# 파일 경로
file_path="./lib/ltdl/ChangeLog.old"

# 'LIBOBJS'를 'AC_LIBOBJ'로 변경
sed -i 's/LIBOBJS/AC_LIBOBJ/g' "$file_path"

echo "$file_path 에서 모든 'LIBOBJS'를 'AC_LIBOBJ'로 변경했습니다."

