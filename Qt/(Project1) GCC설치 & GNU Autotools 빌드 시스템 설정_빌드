# 9. 환경 변수 확인
echo $PATH
/mingw64/x86_64-w64-mingw32/bin:/c/msys64/mingw64/lib/gcc/x86_64-w64-mingw32/14.2.0/include/c++:/c/msys64/mingw64/include:/mingw64/include:/c/libidn2/gettext-0.22.5/libtextstyle/lib:/mingw64/bin:/c/gnulib:/c/msys64/mingw64/bin:/usr/bin:/bin:/usr/share/aclocal:/mingw64/share/aclocal:/c/libidn2/autoconf-2.72/bin:/c/libidn2/libtool-2.4.6:/c/Program Files/Java/jdk-21/bin:/c/Windows/System32:/c/Windows:/c/Windows/System32/Wbem:/c/Windows/System32/WindowsPowerShell/v1.0:/usr/bin/vendor_perl:/usr/bin/core_perl:/c/Users/nayou/AppData/Local/Programs/Python/Python312:/c/Program Files/Java/jdk-21/bin:/c/apache-ant-1.10.15/bin:/c/Users/nayou/vcpkg/downloads/tools/perl/5.38.0.1/c/libexec/gcc/x86_64-w64-mingw32/13.1.0:/c/Users/nayou/vcpkg/downloads/tools/perl/5.38.0.1/c/bin
