# vars="sys_errlist sys_nerr sys_siglist"
vars="sys_errlist sys_nerr"

####################################################################################
# canonicalize_file_name 함수 확인
AC_CHECK_FUNCS([canonicalize_file_name])

# canonicalize_file_name이 없다면 realpath를 사용하도록 정의
if test "x$ac_cv_func_canonicalize_file_name" = "xno"; then
  AC_CHECK_FUNCS([realpath])
fi
####################################################################################
#ifdef HAVE_UNISTD_H
#include <unistd.h>
#endif
int
main ()
{
char *(*pfn) = (char *(*)) canonicalize_file_name
  ;
  return 0;
}
=>
#ifdef HAVE_UNISTD_H
#include <unistd.h>
#endif
#ifdef HAVE_CANONICALIZE_FILE_NAME
  #include <libgen.h>
#else
  #define canonicalize_file_name realpath
#endifint
main ()
{
char *(*pfn) = (char *(*)) canonicalize_file_name
  ;
  return 0;
}

sed -i '/^#ifdef HAVE_UNISTD_H/,/int\s*main\s*(/{
  /int\s*main\s*(/i\
#ifdef HAVE_CANONICALIZE_FILE_NAME\n\
  #include <libgen.h>\n\
#else\n\
  #define canonicalize_file_name realpath\n\
#endif
}' configure
