# grep -r 'LIBOBJS' . \
--exclude-dir='*백업*' \
| cut -d: -f1 \
| grep -v '[0-9]$' \
| grep -v '\.status$' \
| grep -v '\.sh$' \
| grep -v 'back' \
| grep -v 'Binary' \
| grep -v '~' \
| grep -v '\.old' \
| grep -v '\.log' \
| grep -v '\.txt' \
| grep -v '\.ac_sample' \
| grep -v '\.texi' \
| grep -v '\.mk' \
| grep -v '\.gnulib' \
| grep -v '\.in' \
| awk -F'/' '{if (length($NF) > 1 && $NF ~ /\./) print}' \
| sort -u \
| while read file; do
    grep -n 'LIBOBJS' "$file" | sed 's/^[ \t]*//;s/[ \t]*$//' | grep -v '#' | sed "s|^|$file: |"
done
