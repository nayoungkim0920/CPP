(수정)
C:\gcc-14.2.0\lto-plugin\configure.ac
AC_MSG_CHECKING([whether BASE_VERSION is already defined])
if test -z "$BASE_VERSION"; then
    AC_DEFINE([BASE_VERSION], ["14.2.0"], [Define BASE_VERSION if not already defined])
    AC_MSG_RESULT([not defined, set to "14.2.0"])
else
    AC_MSG_RESULT([already defined as $BASE_VERSION])
fi
AC_SUBST(BASE_VERSION)

C:\gcc-14.2.0\lto-plugin\build\config.h
/* Define if the system provides BASE_VERSION. */
#define BASE_VERSION "14.2.0"
