C:\gcc-build\Makefile.in 수정
FLAG_NAME=INCLUDED_BOOTSTRAP_DEBUG
r_src=..
a_src=/c
FILE=/gcc-14.2.0/config/bootstrap-debug.mk

ABS_R_SRC := $(realpath $(r_src))
ABS_A_SRC := $(realpath $(a_src))
ABS_FILE := $(realpath $(r_src)$(FILE))
CYG_ABS_FILE := $(shell cygpath -u "$(ABS_FILE)")

$(info Checking paths...)
$(info Relative path: $(r_src)$(FILE))
$(info Absolute path: $(ABS_A_SRC)$(FILE))
$(info Resolved file path: $(ABS_FILE))
$(info Cygwin converted path: $(CYG_ABS_FILE))

config.status: configure
@if gcc
    ifndef INCLUDED_BOOTSTRAP_DEBUG
        INCLUDED_BOOTSTRAP_DEBUG = 1
        $(info $(FLAG_NAME) defined....)

        ifeq ("$(wildcard $(ABS_FILE))","")
            $(error File $(ABS_FILE) not found!)
        else
            include $(ABS_FILE)
            $(info include $(ABS_FILE)....)
        endif
    else
        $(error $(FLAG_NAME) undefined!)
    endif
@endif gcc

