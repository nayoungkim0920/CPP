~/.bashrc 수정
export PATH="$PATH:/mingw64-backup-11.2.0/bin" # libltdl-7.dll
export C_INCLUDE_PATH="$C_INCLUDE_PATH:/mingw64-backup-11.2.0/include/libltdl" # libltdl
