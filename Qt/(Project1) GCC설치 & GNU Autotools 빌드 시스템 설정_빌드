maybe-clean-fixincludes: clean-fixincludes
EXTRA_HOST_FLAGS = \
	"AR=$(AR)" \
	"AS=$(AS)" \
	"CC=$(CC)" \
	"CXX=$(CXX)" \
	"DLLTOOL=$(DLLTOOL)" \
	"DSYMUTIL=$(DSYMUTIL)" \
	"GFORTRAN=$(GFORTRAN)" \
	"GOC=$(GOC)" \
	"GDC=$(GDC)" \
	"GM2=$(GM2)" \
	"LD=$(LD)" \
	"LIPO=$(LIPO)" \
	"NM=$(NM)" \
	"OBJDUMP=$(OBJDUMP)" \
	"OTOOL=$(OTOOL)" \
	"RANLIB=$(RANLIB)" \
	"READELF=$(READELF)" \
	"STRIP=$(STRIP)" \
	"WINDRES=$(WINDRES)" \
	"WINDMC=$(WINDMC)" \
	"CREATE_GCOV=$(CREATE_GCOV)" \
	"PROFILE_MERGER=$(PROFILE_MERGER)"

COMMON_FLAGS = PROGRAM_VERSION=14.0.2 \
			DESTDIR= \
			RPATH_ENVVAR=PATH \
			lispdir= \
			LDFLAGS=-L/c/libidn2/gettext-0.22.5/libtextstyle/lib \
			oldincludedir=/usr/include \
			LIBCXXFLAGS=-fno-implicit-templates \
			CFLAGS_FOR_BUILD=-I/c/libidn2/gettext-0.22.5/libtextstyle/lib \
			LDFLAGS_FOR_BUILD=-L/c/libidn2/gettext-0.22.5/libtextstyle/lib -Wl,-rpath \
			LIBCFLAGS=-I/c/libidn2/gettext-0.22.5/libtextstyle/lib \
			CFLAGS_FOR_TARGET=-I/c/libidn2/gettext-0.22.5/libtextstyle/lib \
			LDFLAGS_FOR_TARGET=-L/usr/lib -Wl,-rpath \
			target_alias=x86_64-w64-mingw32 \
			SHELL=C:/msys64/usr/bin/sh.exe \
			MAKEINFO=makeinfo

STAGE_FLAGS = STAGE1_CHECKING=--enable-checking=yes,types \
			STAGE1_LANGUAGES=c,c++,lto \
			STAGE1_CFLAGS= \
			STAGE1_CXXFLAGS=-D__USE_MINGW_ACCESS \
			STAGE2_CFLAGS=-D__USE_MINGW_ACCESS -Wno-pedantic-ms-format \
			STAGE2_CXXFLAGS=-D__USE_MINGW_ACCESS -Wno-pedantic-ms-format \
			STAGE3_CFLAGS=-D__USE_MINGW_ACCESS -Wno-pedantic-ms-format \
			STAGE3_CXXFLAGS=-D__USE_MINGW_ACCESS -Wno-pedantic-ms-format

TOOL_FLAGS = AWK=gawk \
			BISON=bison \
			FLEX=flex \
			LEX=flex \
			EXPECT=expect \
			INSTALL=/usr/bin/install \
			INSTALL_DATA=/usr/bin/install \
			INSTALL_PROGRAM=/usr/bin/install \
			INSTALL_SCRIPT=/usr/bin/install \
			M4=/usr/bin/m4 \
			SED=/usr/bin/sed

BASE_FLAGS_TO_PASS = $(COMMON_FLAGS) $(STAGE_FLAGS) $(TOOL_FLAGS)

clean-fixincludes: 
...
$(HOST_EXPORTS); \
...
