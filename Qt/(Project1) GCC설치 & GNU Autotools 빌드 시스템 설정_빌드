(수정)
// fprintf (f, "%u %" PRI_LL "x %s %s\n",
// 	(unsigned int) slot, symtab->aux[j].id,
// 	lto_resolution_str[resolution], 
// 	symtab->syms[j].name);

// PRIx64: uint64_t 타입 데이터를 16진수로 출력.
fprintf(f, "%u %" PRIx64 " %s %s\n",
	(unsigned int) slot, (uint64_t)symtab->aux[j].id,
	lto_resolution_str[resolution], symtab->syms[j].name);

  s = strrchr (name, '.');
  if (s) {
	// sscanf (s, ".%" PRI_LL "x", &obj->out->id);
	// SCNx64: uint64_t 데이터를 16진수로 입력받음.
	sscanf(s, ".%" SCNx64, &obj->out->id);
  }

