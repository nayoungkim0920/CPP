./m4/gnulib-comp.m4 수정
파일로할것
# Like AC_LIBOBJ, except that MODULE_NAME goes into _LT_LIBOBJS instead
# of into LIBOBJS.
#m4_pattern_allow([^_LT_LIBOBJS$])
#_LT_LIBOBJS="$_LT_LIBOBJS $1.$ac_objext"
#])# _LT_LIBOBJS
#if test -n "$_LT_LIBOBJS"; then
#for i in `for i in $_LT_LIBOBJS; do echo "$i"; done | $SED "$_lt_sed_drop_objext" | sort -u`; do
#AC_SUBST([ltdl_LIBOBJS], [$_ltdl_libobjs])
#AC_SUBST([ltdl_LTLIBOBJS], [$_ltdl_ltlibobjs])
# Like AC_LIBOBJ, except that MODULE_NAME goes into _LT_AC_LIBOBJ instead
# of into AC_LIBOBJ.
m4_pattern_allow([^_LT_AC_LIBOBJ$])
_LT_AC_LIBOBJ="$_LT_AC_LIBOBJ $1.$ac_objext"
])# _LT_AC_LIBOBJ
if test -n "$_LT_AC_LIBOBJ"; then
for i in `for i in $_LT_AC_LIBOBJ; do echo "$i"; done | $SED "$_lt_sed_drop_objext" | sort -u`; do
AC_SUBST([ltdl_AC_LIBOBJ], [$_ltdl_libobjs])
AC_SUBST([ltdl_LTAC_LIBOBJ], [$_ltdl_ltlibobjs])

./m4/ltdl.m4 수정
파일로할것
#   m4_pattern_allow([^_LT_LIBOBJS$])
  m4_pattern_allow([^_LT_AC_LIBOBJ$]) # LIBOBJS -> AC_LIBOBJ
#   _LT_LIBOBJS="$_LT_LIBOBJS $1.$ac_objext"
  _LT_AC_LIBOBJ="$_LT_AC_LIBOBJ $1.$ac_objext" # LIBOBJS -> AC_LIBOBJ
# ])# _LT_LIBOBJS
])# _LT_AC_LIBOBJ # LIBOBJS -> AC_LIBOBJ
AC_CONFIG_COMMANDS_PRE([
    _ltdl_libobjs=
    _ltdl_ltlibobjs=
#     if test -n "$_LT_LIBOBJS"; then
    if test -n "$_LT_AC_LIBOBJ"; then # LIBOBJS -> AC_LIBOBJ
      # Remove the extension.
      _lt_sed_drop_objext='s/\.o$//;s/\.obj$//'
#       for i in `for i in $_LT_LIBOBJS; do echo "$i"; done | $SED "$_lt_sed_drop_objext" | sort -u`; do
      for i in `for i in $_LT_AC_LIBOBJ; do echo "$i"; done | $SED "$_lt_sed_drop_objext" | sort -u`; do # LIBOBJS -> AC_LIBOBJ
        _ltdl_libobjs="$_ltdl_libobjs $lt_libobj_prefix$i.$ac_objext"
        _ltdl_ltlibobjs="$_ltdl_ltlibobjs $lt_libobj_prefix$i.lo"
      done
    fi
#     AC_SUBST([ltdl_LIBOBJS], [$_ltdl_libobjs])
    AC_SUBST([ltdl_AC_LIBOBJ], [$_ltdl_libobjs]) # LIBOBJS -> AC_LIBOBJ
#     AC_SUBST([ltdl_LTLIBOBJS], [$_ltdl_ltlibobjs])
    AC_SUBST([ltdl_LTAC_LIBOBJ], [$_ltdl_ltlibobjs]) # LIBOBJS -> AC_LIBOBJ
])
# _LT_LIBOBJ(MODULE_NAME)
# -----------------------
# Like AC_LIBOBJ, except that MODULE_NAME goes into _LT_LIBOBJS instead
# of into LIBOBJS.
AC_DEFUN([_LT_LIBOBJ], [
#   m4_pattern_allow([^_LT_LIBOBJS$])
  m4_pattern_allow([^_LT_AC_LIBOBJ$]) # LIBOBJS -> AC_LIBOBJ
#   _LT_LIBOBJS="$_LT_LIBOBJS $1.$ac_objext"
  _LT_AC_LIBOBJ="$_LT_AC_LIBOBJ $1.$ac_objext" # LIBOBJS -> AC_LIBOBJ
# ])# _LT_LIBOBJS
])# _LT_AC_LIBOBJ # LIBOBJS -> AC_LIBOBJ
AC_CONFIG_COMMANDS_PRE([
    _ltdl_libobjs=
    _ltdl_ltlibobjs=
#     if test -n "$_LT_LIBOBJS"; then
    if test -n "$_LT_AC_LIBOBJ"; then # LIBOBJS -> AC_LIBOBJ
      # Remove the extension.
      _lt_sed_drop_objext='s/\.o$//;s/\.obj$//'
#       for i in `for i in $_LT_LIBOBJS; do echo "$i"; done | $SED "$_lt_sed_drop_objext" | sort -u`; do
      for i in `for i in $_LT_AC_LIBOBJ; do echo "$i"; done | $SED "$_lt_sed_drop_objext" | sort -u`; do # LIBOBJS -> AC_LIBOBJ
        _ltdl_libobjs="$_ltdl_libobjs $lt_libobj_prefix$i.$ac_objext"
        _ltdl_ltlibobjs="$_ltdl_ltlibobjs $lt_libobj_prefix$i.lo"
      done
    fi
#     AC_SUBST([ltdl_LIBOBJS], [$_ltdl_libobjs])
    AC_SUBST([ltdl_AC_LIBOBJ], [$_ltdl_libobjs]) # LIBOBJS -> AC_LIBOBJ
#     AC_SUBST([ltdl_LTLIBOBJS], [$_ltdl_ltlibobjs])
    AC_SUBST([ltdl_LTAC_LIBOBJ], [$_ltdl_ltlibobjs]) # LIBOBJS -> AC_LIBOBJ
])

./m4/non-recursive-gnulib-prefix-hack.m4 수정
#   gl_LIBOBJS=`echo "$gl_LIBOBJS" | sed -e 's, , $1/,g'`
  gl_AC_LIBOBJ=`echo "$gl_AC_LIBOBJ" | sed -e 's, , $1/,g'` # LIBOBJS -> AC_LIBOBJ

