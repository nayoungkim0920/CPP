#libltdl_la_LIBADD	= $(LTLIBOBJS) # LIBOBJS -> AC_LIBOBJ
#libltdl_la_DEPENDENCIES = $(LT_DLLOADERS) $(LTLIBOBJS) # LIBOBJS -> AC_LIBOBJ
