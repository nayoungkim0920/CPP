# Like AC_LIBOBJ, except that the module name goes
# into lts_LIBOBJS instead of into LIBOBJS.
AC_DEFUN([lts_LIBOBJ], [
  AS_LITERAL_IF([$1], [lts_LIBSOURCES([$1.c])])dnl
#   lts_LIBOBJS="$lts_LIBOBJS $1.$ac_objext"
  lts_AC_LIBOBJ="$lts_AC_LIBOBJ $1.$ac_objext" # LIBOBJS -> AC_LIBOBJ
])
#   ltstests_LIBOBJS="$ltstests_LIBOBJS $1.$ac_objext"
  ltstests_AC_LIBOBJ="$ltstests_AC_LIBOBJ $1.$ac_objext" # LIBOBJS -> AC_LIBOBJ

