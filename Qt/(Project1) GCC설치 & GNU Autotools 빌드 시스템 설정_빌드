export PATH="$PATH:/c/Program Files (x86)/Microsoft Visual Studio/2019/Community/VC/Tools/MSVC/14.29.30133/bin/Hostx64/x64" # dumpbin.exe

(오류)
checking for argz_add... no
checking for argz_append... no
checking for argz_count... no
checking for argz_create_sep... no
