-rw-r--r-- 1 nayou nayou 131772 10월 31 11:36 libargz.a

configure.ac
m4_pattern_allow([gl_ARGZ])
gl_ARGZ
gl_MODULES([argz])
AC_CONFIG_HEADERS([config.h lib/argz.h])
AC_CHECK_HEADERS([argz.h], [
  AM_CONDITIONAL([HAVE_SYSTEM_ARGZ_H], [true])
], [
  AM_CONDITIONAL([HAVE_SYSTEM_ARGZ_H], [false])
])
AC_SUBST([ARGZ_H], ["lib/argz.h"])
AC_SUBST([LIBARGZ], [libargz.la])
LIBS="$LIBS -liberty -ltextstyle"
AC_SUBST([LIBS])
AC_DEFINE([GL_GENERATE_ARGZ_H], [1], [Define if we want to generate argz.h])
AC_CHECK_FUNCS([argz_add argz_append argz_count argz_create_sep argz_insert argz_next argz_stringify])
m4_ifdef([gl_ARGZ], [gl_ARGZ])
AC_CHECK_LIB([argz], [argz_create_sep], [], [AC_MSG_ERROR([argz library not found])])
AC_LIBOBJ([lt__argz])
AC_LIBOBJ([argz])

Makefile.am
lib_LTLIBRARIES = libtextstyle.la libltdlc.la # libargz.la 제거 중복선언
noinst_LTLIBRARIES = libargz.la 
libargz_la_SOURCES = lib/argz.c
libltdlc_la_SOURCES = libltdl/lt__argz.c
LIBS = -liberty -ltextstyle
AM_LIBS = $(ITL_LIBS) $(NCURSES_LIBS) $(GLIB_LIBS) $(LIBS)  # LIBS 포함
libtextstyle_la_SOURCES = \ #라이브러리에 포함될 소스 파일 목록을 나열
	lib/color.c \
	lib/gl_array_list.c \
	lib/term-ostream.c \
	lib/ostream.c \
	lib/concat-filename.c \
	lib/xconcat-filename.c \
    lib/xsize.c \
	lib/error.c \
	lib/full-write.c \
	lib/term-style-control.c \
	lib/fsync.c \
	lib/xstrdup.c \
	lib/xgethostname.c \
	lib/argz.c \
	$(GNULIB_SRCS)

# Here is where to add the argz module
# Begin Gnulib module argz
# BUILT_SOURCES = lib/argz.h
# EXTRA_DIST = lib/argz.in.h lib/argz.h.in # 중복정의

if GL_GENERATE_ARGZ_H
lib/argz.h: lib/argz.in.h $(top_builddir)/config.status
	$(gl_V_at)$(SED_HEADER_TO_AT_t) $(srcdir)/lib/argz.in.h
	$(AM_V_at)mv $@-t $@
else
lib/argz.h: $(top_builddir)/config.status
	rm -f $@
endif

MOSTLYCLEANFILES = lib/argz.h lib/argz.h-t

# Conditionally include argz.c
if GL_COND_OBJ_ARGZ
libtextstyle_la_SOURCES += lib/argz.c
endif

# End Gnulib module argz

libtextstyle_la_LIBS = -lncurses $(GNULIB_LIBS) $(GL_LIBOBJS) $(LTDLIBS) $(LT_LIBOBJS) -lltdl -largz # libtextstyle 라이브러리에 링크할 라이브러리들

libltdlc_la_SOURCES += \
  libltdl/lt__alloc.c \
  libltdl/lt__argz.c \
  libltdl/lt_dlloader.c \
  libltdl/lt_error.c \
  libltdl/ltdl.c \
  libltdl/slist.c
#AC_LIBOBJ([lt__argz]) # configure.ac에서 선언
# Add this rule to copy argz.in.h to argz.h
#lib/argz.h: lib/argz.h.in
#	cp lib/argz.h.in lib/argz.h

EXTRA_DIST = \
  lib/argz.in.h \
  lib/argz.h.in \

lib/Makefile.am
## begin gnulib module argz

if GL_GENERATE_ARGZ_H
# GL_GENERATE_ARGZ_H가 참일 때 실행할 명령어 또는 변수 설정
BUILT_SOURCES += lib/argz.h
lib/argz.h: lib/argz.in.h $(top_builddir)/config.status
    $(gl_V_at)$(SED_HEADER_TO_AT_t) $(srcdir)/lib/argz.in.h
    $(AM_V_at)mv $@-t $@
else
# GL_GENERATE_ARGZ_H가 거짓일 때 실행할 명령어 또는 변수 설정
lib/argz.h: $(top_builddir)/config.status
    rm -f $@
endif

MOSTLYCLEANFILES += argz.h argz.h-t

# Conditionally compile argz.c if GL_COND_OBJ_ARGZ is defined
if GL_COND_OBJ_ARGZ
libgnu_a_SOURCES += argz.c
endif

## end gnulib module argz

