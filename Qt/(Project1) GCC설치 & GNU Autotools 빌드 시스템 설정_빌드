struct any_ostream_representation {
    int is_windows_console;               // 윈도우 콘솔 여부를 나타내는 플래그
    int current_console_attributes;       // 현재 콘솔 속성 (색상, 스타일 등)
    int default_console_attributes;       // 기본 콘솔 속성
    HANDLE handle;                        // 콘솔 핸들 (윈도우에서 사용)
}

