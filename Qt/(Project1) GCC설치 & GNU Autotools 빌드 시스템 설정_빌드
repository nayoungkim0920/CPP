cp /c/msys64/mingw64/bin/strip.exe /c/msys64/mingw64/bin/x86_64-w64-mingw32-strip.exe
cp /c/msys64/mingw64/bin/ranlib.exe /c/msys64/mingw64/bin/x86_64-w64-mingw32-ranlib.exe

