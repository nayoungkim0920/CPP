../Makefile.in 에서 libiberty.a 경로 확인
"C:\gcc-14.2.0\lto-plugin\Makefile.in"
478 : liblto_plugin.la: $(liblto_plugin_la_OBJECTS) $(liblto_plugin_la_DEPENDENCIES) $(EXTRA_liblto_plugin_la_DEPENDENCIES) 
         $(AM_V_GEN)$(liblto_plugin_la_LINK) -rpath $(libexecsubdir) $(liblto_plugin_la_OBJECTS) $(liblto_plugin_la_LIBADD) $(LIBS)
=> liblto_plugin_la_LINK : C:/msys64/usr/bin/sh.exe ./libtool --tag=CC --tag=disable-static --mode=link x86_64-w64-mingw32-gcc -Wall -DBASE_VERSION="14.2.0" -I/c/msys64/mingw64/include -I/mingw64/include -I/c/libidn2/gettext-0.22.5/libtextstyle/lib -O2 -Wc,-static-libgcc -no-undefined -bindir /mingw64/bin -module -avoid-version -bindir /mingw64/libexec/gcc/x86_64-w64-mingw32/14.2.0 -Wl,--version-script=../lto-plugin.map -Wc,../libiberty/libiberty.a -L/mingw64/lib/gcc/x86_64-w64-mingw32/14.2.0 -L/mingw64/lib -L/usr/lib -L/mingw64/x86_64-w64-mingw32/lib -L/c/libidn2/gettext-0.22.5/libtextstyle/lib -Wl,-rpath,/mingw64/bin -o liblto_plugin.la
=> liblto_plugin_la_LINK값 안에 ../libiberty/libiberty.a 이 값을 /c/gcc-14.2.0/libiberty/build/libiberty.a 수정해야함
Makefile.in 검색
liblto_plugin_la_LINK 
=> liblto_plugin_la_LINK = $(LIBTOOL) --tag=CC $(AM_LIBTOOLFLAGS) \
	$(LIBTOOLFLAGS) --mode=link $(CCLD) $(AM_CFLAGS) $(CFLAGS) \
	$(liblto_plugin_la_LDFLAGS) $(LTLDFLAGS) -o $@
liblto_plugin_la_LDFLAGS
=> liblto_plugin_la_LDFLAGS = $(AM_LDFLAGS) $(lt_host_flags) -module \
	-avoid-version -bindir $(libexecsubdir) $(version_arg) $(if \
	$(wildcard $(libiberty_noasan)),, $(if $(wildcard \
	$(libiberty_pic)),,-Wc,$(libiberty)))
libiberty
=> libiberty = $(with_libiberty)/libiberty.a
with_libiberty
=> with_libiberty = @with_libiberty@

C:\gcc-14.2.0\lto-plugin\configure.ac 수정
AC_ARG_WITH(libiberty,
  [AS_HELP_STRING([--with-libiberty=PATH],
    [specify the directory where to find libiberty [/c/gcc-14.2.0/libiberty/build]])],
  [], with_libiberty=/c/gcc-14.2.0/libiberty/build)
AC_SUBST(with_libiberty)
autoconf 명령어로 configure를 다시 생성
C:\gcc-14.2.0\lto-plugin\configure 파일 확인
# Check whether --with-libiberty was given.
if test "${with_libiberty+set}" = set; then :
  withval=$with_libiberty;
else
  with_libiberty=/c/gcc-14.2.0/libiberty/build
fi
빌드
nayou@kimnayooung MINGW64 /c/gcc-14.2.0/lto-plugin
# cd build
nayou@kimnayooung MINGW64 /c/gcc-14.2.0/lto-plugin/build
# ../configure
C:\gcc-14.2.0\lto-plugin\build\Makefile 파일 확인
with_libiberty = /c/gcc-14.2.0/libiberty/build
성공!!

nayou@kimnayooung MINGW64 /c/gcc-14.2.0/lto-plugin/build
