## diffseq가 필요하면 diffseq.c 추가 (diffmark -> diffseq로 변경)
if GL_GENERATE_DIFFSEQ
  DIFFSEQ_SOURCES = diffseq.c # diffseq설치
endif

## GL_COND_OBJ_DIFFSEQ가 참이면 diffseq.o 객체 추가 (diffmark -> diffseq로 변경)
if GL_COND_OBJ_DIFFSEQ
  OBJ_DIFFSEQ = diffseq.o # diffseq설치
endif

