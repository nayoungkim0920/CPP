"C++ 컴파일"
cpp_test.cpp
#include <iostream>
#include <optional>

static_assert(__cplusplus >= 201703L, "C++17 is not supported");

int main() {
    std::cout << "C++17 is supported!" << std::endl;
    return 0;
}

