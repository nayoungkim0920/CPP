# ar 플래그를 설정하여 'u' 플래그를 제거
AR_FLAGS = rcs

# 정적 라이브러리 정의
noinst_LIBRARIES = libiberty.a

# 라이브러리에 포함될 소스 파일
libiberty_a_SOURCES = \
    argv.c \
    cplus-dem.c \
    dyn-string.c \
    fibheap.c \
    floatformat.c \
    fnmatch.c \
    getruntime.c \
    hex.c \
    lbasename.c \
    make-relative-prefix.c \
    objalloc.c \
    obstack.c \
    partition.c \
    pexecute.c \
    physmem.c \
    safe-ctype.c \
    simple-object.c \
    strerror.c \
    strsignal.c \
    xatexit.c \
    xexit.c \
    xmalloc.c \
    xstrdup.c

# 라이브러리 빌드 규칙 수정 (ar 명령어에 AR_FLAGS 적용)
libiberty.a: $(libiberty_a_OBJECTS)
	$(AR) $(AR_FLAGS) $@ $(libiberty_a_OBJECTS)

# 추가 규칙
