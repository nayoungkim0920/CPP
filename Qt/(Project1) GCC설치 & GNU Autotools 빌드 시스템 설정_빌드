  m4_pattern_allow([^gl_AC_LIBOBJ$])dnl a variable # LIBOBJS -> AC_LIBOBJ
#   m4_pattern_allow([^gl_LTLIBOBJS$])dnl a variable
  m4_pattern_allow([^gl_LTAC_LIBOBJ$])dnl a variable # LIBOBJS -> AC_LIBOBJ
#  if test $HAVE_ISNANL = 0 || test $REPLACE_ISNAN = 1; then   # LIBOBJS -> AC_LIBOBJ
