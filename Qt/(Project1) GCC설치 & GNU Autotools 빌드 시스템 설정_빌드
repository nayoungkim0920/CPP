# echo $CXXFLAGS
-std=c++17 -I/c/gnulib/lib -I/mingw64/include -O2 -DDEBUG -DVERSION="1.0.0"

nayou@kimnayooung MINGW64 /c/libidn2/gettext-0.22.5/libtextstyle
# echo $CFLAGS
-I/c/gnulib/lib -I/mingw64/include -O2

