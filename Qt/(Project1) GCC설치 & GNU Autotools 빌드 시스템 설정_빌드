AC_DEFUN([CHECK_WINDOWS_CONSOLES], [
  AC_MSG_CHECKING([for Windows consoles])
  if test -f /mingw64/include/windows.h; then
    AC_DEFINE([HAVE_WINDOWS_CONSOLES], [1], [Define to 1 if you have Windows consoles.])
    AC_MSG_RESULT([yes])
  else
    AC_MSG_RESULT([no])
  fi
])

