#ocv_clear_vars(HAVE_ONNX) #주석처리
# if(NOT HAVE_ONNX) 주석처리
#  ocv_clear_vars(HAVE_ONNX ORT_LIB ORT_INCLUDE_DIR)
# endif()
# 추가
if(NOT HAVE_ONNX)
  # 수동으로 변수 초기화
  set(HAVE_ONNX)
  set(ORT_LIB)
  set(ORT_INCLUDE_DIR)
endif()
