#    lt_dlloader$(EXEEXT) \
#    lt_error$(EXEEXT) \
#    ltdl$(EXEEXT) \
#    slist$(EXEEXT)
	
libltdlc_la_LIBS = -lncurses $(GNULIB_LIBS) $(GL_LIBOBJS) $(LTDLIBS) $(LT_LIBOBJS)
libltdlc_la_LIBADD = -lltdl
libltdlc_la_LDFLAGS = -shared -Wl,--out-implib=libltdlc.dll.a

