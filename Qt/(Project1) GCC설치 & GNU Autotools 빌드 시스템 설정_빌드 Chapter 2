  -no-recursion | --no-recursion | --no-recursio | --no-recursi   | --no-recurs | --no-recur | --no-recu | --no-rec | --no-re | --no-r)
    no_recursion=yes ;;

  -oldincludedir | --oldincludedir | --oldincludedi | --oldincluded   | --oldinclude | --oldinclud | --oldinclu | --oldincl | --oldinc   | --oldin | --oldi | --old | --ol | --o)
