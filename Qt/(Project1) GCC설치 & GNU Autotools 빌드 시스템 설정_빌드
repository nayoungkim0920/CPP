typedef struct term_style_control_data {
    int volatile fd;
    ttyctl_t volatile tty_control;     /* 신호 처리 및 tty 제어 */
    #if HAVE_TCGETATTR
    bool volatile same_as_stderr;
    #endif
    bool non_default_active;           /* activate_term_non_default_mode() 가 활성화된 경우 */
} term_style_control_data;

