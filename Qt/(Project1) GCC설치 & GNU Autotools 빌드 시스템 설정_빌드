(빌드)
{ autopoint --force; } 2>&1 | tee ./log/autopoint.log && { rm -f po/Makefile.in.in; } && { rm -rf autom4te.cache; } && { aclocal -I m4 -I gnulib-m4 -I /usr/share/aclocal; } 2>&1 | tee ./log/aclocal.log && { libtoolize --copy --force --verbose; } 2>&1 | tee ./log/libtoolize.log && { cp /usr/share/aclocal/ltdl.m4 ./m4/; } && { autoconf --verbose; } 2>&1 | tee ./log/autoconf.log && { autoheader; } 2>&1 | tee ./log/autoheader.log && { automake --add-missing --force-missing; } 2>&1 | tee ./log/automake.log && { ./configure --verbose --host=x86_64-w64-mingw32; } 2>&1 | tee ./log/configure.log

