# Check for specific features and set conditionals
AM_CONDITIONAL([GL_GENERATE_HEADERS], [test "$gl_generate_headers" = "yes"])
AM_CONDITIONAL([GL_GENERATE_DIFFMARK], [test "$gl_generate_diffmark" = "yes"])
AM_CONDITIONAL([GL_GENERATE_NANOSLEEP], [test "$gl_generate_nanosleep" = "yes"])
AM_CONDITIONAL([GL_GENERATE_OBSTACK], [test "$gl_generate_obstack" = "yes"])
