# pkg-config 경로 설정
export PKG_CONFIG_PATH="/mingw64/lib/pkgconfig"
export PKG_CONFIG_PATH="$PKG_CONFIG_PATH:/usr/lib/pkgconfig"
# library 경로 설정
export LD_LIBRARY_PATH="/mingw64/lib"
export LD_LIBRARY_PATH="$LD_LIBRARY_PATH:/mingw64/bin"

