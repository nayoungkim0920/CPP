configure.ac 수정
# PKG_CHECK_MODULES(
#     [GLIB], 
#     [glib-2.0 gobject-2.0], 
#     [have_glib=yes], 
#     [have_glib=no]
# )

PKG_CHECK_MODULES([GLIB], [glib-2.0 >= 2.10.0 gobject-2.0 >= 2.10.0], 
    [have_glib=yes], 
    [have_glib=no])

if test "x$have_glib" = "xno"; then
    AC_MSG_ERROR([GLib 2.10.0 or higher is required])
fi

AC_SUBST([GLIB_CFLAGS])
AC_SUBST([GLIB_LIBS])

