libtoolize --copy --force && aclocal --force && autoconf && automake --add-missing --copy && autoreconf -ivf && 
sed -i 's/ -V -qversion -version/ --version/g' configure 
sed -i 's/ -V -qversion/ --version/g' configure 
sed -i 's/$NM -V/$NM --version/g' configure 
sed -i 's/$CC -V/$CC --version/g' configure
sed -i '/^ac_save_CC=\$CC/i ac_cv_prog_cc_c23=no' configure 
sed -i 's/\$ac_c_conftest_c23_program//g' configure 
sed -i '/ac_fn_c_check_header_mongrel.*"minix\/config.h"/,/^  fi/d' configure 
sed -i 's/cxx11test::delegate d2();/cxx11test::delegate d2{};/g' configure
sed -i '/^ VERSION='\''1\.0'\''$/d' configure 
sed -i "/# define ISLOWER/c\#define ISLOWER(c) (islower(c))" configure
sed -i "/# define TOUPPER/c\#define TOUPPER(c) (toupper(c))" configure 
sed -i '/&& LARGE_FTYPE % 2147483647 == 1)/d' configure
sed -i '/int FTYPE_is_large\[/c\
void check_large_ftype() {\
    int FTYPE_is_large[(LARGE_FTYPE % 2147483629 == 721 && LARGE_FTYPE % 2147483647 == 1) ? 1 : -1];\
}' configure 
sed -i '/\? 1 : -1];/d' configure 
sed -i 's/\bnot\b/!/g' configure 
sed -i 's/! big endian/#error "not big endian"/g' configure 
sed -i 's/! a universal capable compiler//g' configure 
sed -i 's/! big endian/#if BYTE_ORDER != BIG_ENDIAN\n    #error "not big endian"\n#endif/g' configure 
sed -i '/^#define VERSION "\$VERSION"$/d' configure 
sed -i '/old_archive_cmds_CXX=/s/-u//g' configure && 
./configure &&  make

