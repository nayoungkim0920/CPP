# unistr 기능이 있는지 확인
AC_ARG_ENABLE([unicase],
  [AS_HELP_STRING([--enable-unicase], [Enable unicase support])],
  [enable_unicase=yes], [enable_unicase=no])
# enable_unicase가 yes이면 조건부 매크로 정의
if test "$enable_unicase" = yes; then
  AC_DEFINE([LIBUNISTRING_COMPILE_UNICTYPE_CTYPE_ALPHA], [1], [Enable unicase alpha])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNICTYPE_CTYPE_BLANK], [1], [Enable unicase blank])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNICTYPE_CTYPE_CNTRL], [1], [Enable unicase control])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNICTYPE_CTYPE_DIGIT], [1], [Enable unicase digit])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNICTYPE_CTYPE_GRAPH], [1], [Enable unicase graph])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNICTYPE_CTYPE_LOWER], [1], [Enable unicase lower])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNICTYPE_CTYPE_PRINT], [1], [Enable unicase print])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNICTYPE_CTYPE_PUNCT], [1], [Enable unicase punct])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNICTYPE_CTYPE_SPACE], [1], [Enable unicase space])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNICTYPE_CTYPE_UPPER], [1], [Enable unicase upper])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNICTYPE_CTYPE_XDIGIT], [1], [Enable unicase xdigit])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNISTR_U8_UCTOMB], [1], [Enable unistr u8 uctomb])
  AC_DEFINE([LIBUNISTRING_COMPILE_UNIWIDTH_WIDTH], [1], [Enable uniwidht width])
fi

