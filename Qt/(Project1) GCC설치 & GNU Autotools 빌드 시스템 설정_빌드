# relocatable-ld 옵션을 사용할 수 있게 설정
AC_ARG_ENABLE([relocatable-ld],
  [AS_HELP_STRING([--enable-relocatable-ld], [Enable relocatable LD])],
  [relocatable_ld=$enableval], [relocatable_ld=no])

# relocatable-ld 옵션이 활성화된 경우에만 RELOCATABLE_VIA_LD 정의
if test "x$relocatable_ld" = "xyes"; then
  AM_CONDITIONAL([RELOCATABLE_VIA_LD], [true])
  AC_DEFINE([RELOCATABLE_VIA_LD], [1], [Define if we use relocatable LD])
else
  AM_CONDITIONAL([RELOCATABLE_VIA_LD], [false])
fi
