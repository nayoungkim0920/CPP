find /c/gcc-build -type d -iname "*texinfo*"
검색결과없음

nayou@kimnayooung MINGW64 /c/gcc-build
# for dir in texinfo/doc texinfo/info texinfo/intl texinfo/lib; do [ -d "$dir" ] && echo "디 렉터리: $dir" && ls -l "$dir" || echo "디렉터리 없음: $dir"; done
디렉터리 없음: texinfo/doc
디렉터리 없음: texinfo/info
디렉터리 없음: texinfo/intl
디렉터리 없음: texinfo/lib

list_dirs.sh 생
Bash 스크립트
#!/bin/bash

# 디렉터리 목록
dirs=("texinfo/doc" "texinfo/info" "texinfo/intl" "texinfo/lib")

# 디렉터리 확인 및 파일 목록 출력
for dir in "${dirs[@]}"; do
  if [ -d "$dir" ]; then
    echo "디렉터리: $dir"
    ls -l "$dir"  # 자세한 파일 목록 출력
    echo
  else
    echo "디렉터리 없음: $dir"
  fi
done

